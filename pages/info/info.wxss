.cont {
  height: 100%;
  width: 100%;
  background: #fff;
}

.xing {
  margin-left: 10px;
  height: 44px;
  background: #fff;
  text-align: left;
  line-height: 44px;
  font-size: 14px;
  width: 100%;
  color: #303030;
}

.placeholder {
  color: #c1c1c1;
  font-size: 14px;
}

.in {
  float: right;
  width: 70%;
  padding-top: 0rpx;
  font-size: 14px;
  height: 44px;
  line-height: 44px;
}

.su {
  background: #27aec1;
  width: 80%;
  margin: 0 auto;
  color: #fff;
}

container {
  position: absolute;
  height: 100%;
  width: 100%;
  min-height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 0;
  box-sizing: border-box;
  background-color: #f8f8f8;
}

.citypickers {
  position: fixed;
  height: 100%;
  width: 100%;
  min-height: 100%;
  background-color: red;
}

.citybody {
  position: fixed;
  bottom: 0px;
}

.cityheader {
  position: absolute;
  top: 0px;
  width: 100%;
  z-index: 4;
}

.city-cancel {
  float: left;
  margin: 23rpx;
  color: #818181;
}

.city-true {
  float: right;
  margin: 23rpx;
  color: #2fb42e;
}

.section .picker {
  background-color: #fff;
  border-bottom: 1px #d9d9d9 solid;
  border-top: 1px #d9d9d9 solid;
  padding: 20rpx;
}
